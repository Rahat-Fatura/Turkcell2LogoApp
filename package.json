{
  "name": "rs-turkcell-to-logo",
  "version": "0.3.4",
  "description": "An app for turkcell invoices to logo invoices",
  "scripts": {
    "start": "pm2 start ecosystem.config.json --no-daemon",
    "dev": "cross-env NODE_ENV=development nodemon src/index.js",
    "build:dev": "webpack --config build/webpack.dev.conf.js",
    "build:prod": "webpack --config build/webpack.dev.conf.js",
    "test": "cross-env NODE_ENV=test node tests/current.test.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check **/*.js",
    "prettier:fix": "prettier --write **/*.js",
    "prisma:lint": "prisma-lint",
    "prisma:migrate": "prisma migrate dev",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "prisma:seed": "prisma db seed --preview-feature",
    "prisma:push": "prisma db push --preview-feature",
    "prisma:reset": "prisma migrate reset --preview-feature"
  },
  "dependencies": {
    "@prisma/client": "^4.11.0",
    "axios": "^1.6.2",
    "axios-retry": "^4.0.0",
    "bytes": "^3.1.2",
    "config": "^3.3.10",
    "cookie-parser": "~1.4.4",
    "date-and-time": "^3.1.0",
    "debug": "~2.6.9",
    "dotenv": "^16.3.1",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "helmet": "^7.1.0",
    "http-errors": "~1.6.3",
    "http-status": "^1.7.3",
    "joi": "^17.11.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "morgan": "~1.9.1",
    "mssql": "^10.0.2",
    "node-cron": "^3.0.3",
    "ubl2json": "^2.0.2",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^8.55.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-security": "^1.7.1",
    "javascript-obfuscator": "^4.1.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.0.2",
    "pm2": "^5.3.0",
    "prettier": "^3.1.0",
    "prisma": "^4.11.0",
    "webpack": "^5.90.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0",
    "webpack-obfuscator": "^3.5.1"
